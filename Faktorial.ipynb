      "source": [
        "def faktorial(n):\r\n",
        "  a = 1\r\n",
        "  for i in range(1, n+1):\r\n",
        "    a*=i\r\n",
        "    if i == n:\r\n",
        "      print(i, end=\" = \")\r\n",
        "    else:\r\n",
        "      print(i, end=\"*\")\r\n",
        "  return a\r\n",
        "\r\n",
        "fak = int(input(\"Masukkan jumlah faktorial : \"))\r\n",
        "print(faktorial(fak))\r\n",
        "\r\n"
      ],
